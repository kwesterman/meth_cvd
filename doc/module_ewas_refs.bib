journal = {Epigenetics},
keywords = {Adipose tissue,DNA methylation,FASN (fatty acid synthase),HIF3A (hypoxia inducible factor 3,blood,body mass index,epigenomics,methylation,principal component analysis,surrogate markers},
mendeley-tags = {methylation},
