title = {{Epigenome-wide association study reveals differential DNA methylation in individuals with a history of myocardial infarction}},
url = {https://watermark.silverchair.com/ddw302.pdf?token=AQECAHi208BE49Ooan9kkhW{\_}Ercy7Dm3ZL{\_}9Cf3qfKAc485ysgAAAaQwggGgBgkqhkiG9w0BBwagggGRMIIBjQIBADCCAYYGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQM4YHjkl6LbTjPhrE-AgEQgIIBV8pAiYdS93l4iEiABHsqofjoq6Fj32f53WsS-zPCsy-Y-Ry3},
