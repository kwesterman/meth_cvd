pages = {e1006105},
pmid = {27336614},
title = {{Epigenome-wide Association Studies and the Interpretation of Disease -Omics}},
url = {http://journals.plos.org/plosgenetics/article/file?id=10.1371/journal.pgen.1006105{\&}type=printable http://dx.plos.org/10.1371/journal.pgen.1006105},
volume = {12},
year = {2016}
}
