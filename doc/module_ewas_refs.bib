title = {{eFORGE: A Tool for Identifying Cell Type-Specific Signal in Epigenomic Data}},
url = {https://www.sciencedirect.com/science/article/pii/S2211124716314796?via{\%}3Dihub http://linkinghub.elsevier.com/retrieve/pii/S2211124716314796},
