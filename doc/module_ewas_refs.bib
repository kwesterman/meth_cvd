doi = {10.1001/JAMACARDIO.2018.0510},
file = {:Users/kennywesterman/Library/Application Support/Mendeley Desktop/Downloaded/Aslibekyan et al. - 2018 - Association of Methylation Signals With Incident Coronary Heart Disease in an Epigenome-Wide Assessment of C.pdf:pdf},
